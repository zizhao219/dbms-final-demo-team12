                          <th>Wed.</th>
                          <th>Thu.</th>
                          <th>Fri</th>
                       </tr>
                       <tr>
                          <th>9:00~12:00</th>

                     <% if(schedule.Mon_9_12 ==1){%>
                       <th>✓</th>

                     <%} else{%>
                       <th></th>
                       <%}%>
                     <% if(schedule.Tue_9_12 ==1){%>
                       <th>✓</th>
                       <%} else{%>
                        <th></th>

                    <%}%>
                    <% if(schedule.Wed_9_12 ==1){%>
                      <th>✓</th>
                      <%} else{%>
                        <th></th>

                    <%}%>
                    <% if(schedule.Thu_9_12 ==1){%>
                      <th>✓</th>
                      <%} else{%>
                        <th></th>

                    <%}%>
                    <% if(schedule.Fri_9_12 ==1){%>
                      <th>✓</th>
                      <%} else{%>
                      <th></th>

                    <%}%>

                    </tr>
                    <tr>
                       <th>13:00~17:00</th>


                     <% if(schedule.Mon_13_17 ==1){%>
                       <th>✓</th>
                       <%} else{%>
                         <th></th>

                      <%}%>
                     <% if(schedule.Tue_13_17==1){%>
                       <th>✓</th>
                       <%} else{%>
                         <th></th>

                     <%}%>
                     <% if(schedule.Wed_13_17==1){%>
                       <th>✓</th>
                       <%} else{%>
                         <th></th>

                     <%}%>
                     <% if(schedule.Thu_13_17==1){%>
                       <th>✓</th>
                       <%} else{%>
                         <th></th>
                     <%}%>
                     <% if(schedule.Fri_13_17==1){%>
                       <th>✓</th>
                       <%} else{%>
                         <th></th>
                         <%}%>
                   </tr>
                   </table>
                   </div>
                   <hr>
             <%  }); %>
               <%}}%>
               </div>
           </div>
       </div>


