<% include ./header %>

<header class="intro-header" style="background-image: url('/images/旗袍.jpg')">
       <div class="container">
           <div class="row">
               <div class="col-lg-8 col-lg-offset-2 col-md-10 col-md-offset-1">
                   <div class="post-heading">
                       <h1>All  &nbsp; Members'  &nbsp;Schedules</h1>

                   </div>
               </div>
           </div>
       </div>
   </header>

   <!-- Post Content -->

       <div class="container">
           <div class="row">
               <div class="col-lg-8 col-lg-offset-2 col-md-10 col-md-offset-1">
                <%if(global.member){%>
                 <%if(schedulelist){%>
                   <%schedulelist.forEach(function(schedule){%>
