               </table>
               </div>
           </div>

       </div>
       <%if(global.member){%>
         <div class="col-lg-4 col-lg-offset-5 col-md-10 col-md-offset-1">
       <form action ="/schedule_show/show" method="POST">
        <button type="submit" class="btn btn-default">Show All Schedule</button>

       </form>
