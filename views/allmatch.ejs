                 <%if(rowlist){%>
                   <%if(rowlist.length==0){%>
                     <h2 class="post-title">
                       NO People
                     </h2>
                  <% } else {%>
                  <%for(;rowlist.length!=0;){%>
                   <div class="post-preview">
                      <% var sth =rowlist.pop()%>
