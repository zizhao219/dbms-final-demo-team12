                        <%}%>
                        <% if(sth[0].Thu_9_12 ==1){%>
                          <th>✓</th>
                          <%} else{%>
                            <th></th>

                        <%}%>
                        <% if(sth[0].Fri_9_12 ==1){%>
                          <th>✓</th>
                          <%} else{%>
